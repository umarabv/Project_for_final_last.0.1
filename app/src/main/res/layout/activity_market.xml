        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:layout_width="338dp"
        android:layout_height="106dp"
        android:layout_marginStart="36dp"
